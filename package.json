{
  "name": "fundamentum",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "del-cli dist/main.js",
    "lint": "eslint src/**.js",
    "watch:lint": "esw --watch src/**.js",
    "watch:scss": "onchange \"src/scss/*.scss\" -- npm run build:css",
    "build:css": "node-sass src/scss -o dist/css",
    "build:dev": "npm run clean && webpack --mode development",
    "build:prod": "webpack --mode production",
    "start-dev": "npm-run-all --parallel watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlfnkls/fundamentum.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wlfnkls/fundamentum/issues"
  },
  "homepage": "https://github.com/wlfnkls/fundamentum#readme",
  "dependencies": {
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-watch": "^3.1.4",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  }
}